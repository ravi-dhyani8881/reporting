  <select id="selectAllIphoneLabels" resultMap="CustomizedBaseResultMap">
	   		<![CDATA[ 
	   		SELECT lk.label_key , lk.label_value as value
			FROM label_keys AS lk
			WHERE lk.tab_type = 'iphone' 
			]]>
  		</select>

	     <select id="selectAllWebLabels" resultMap="CustomizedBaseResultMap">
	     	<![CDATA[ 
	  		SELECT lk.label_key , lk.label_value as value
			FROM label_keys AS lk
			WHERE lk.tab_type <> 'iphone' 
			]]>
  		</select>
  
   <select id="selectAllIphoneLabelsByMerchantId" resultMap="CustomizedDateAddedBaseResultMap" parameterClass="long">
	   		<![CDATA[ 
	   		SELECT lk.label_key ,COALESCE(l.modified_text, lk.label_value) as value, l.date_added
			FROM labels AS l, label_keys AS lk
			WHERE lk.label_key_id = l.label_key_id
			AND lk.tab_type = 'iphone' 
			AND merchant_id = #value#
			]]>
  </select>
  
     <select id="selectAllWebLabelsByMerchantId" resultMap="CustomizedBaseResultMap" parameterClass="long">
     	<![CDATA[ 
  		SELECT lk.label_key ,COALESCE(l.modified_text, lk.label_value) as value
		FROM labels AS l, label_keys AS lk
		WHERE lk.label_key_id = l.label_key_id
		AND lk.tab_type <> 'iphone' 
		AND merchant_id = #value#
		]]>
 	 </select>
  
   		
	 <select id="isLabelInSync" resultMap="CustomizedCountBaseResultMap" parameterClass="com.mobicart.model.Labels">
     	<![CDATA[ 
     		select count(*) as count from labels WHERE merchant_id = #merchantId:BIGINT# AND DATE_FORMAT(date_added, '%Y-%m-%d %H %i')  = DATE_FORMAT(#dateAdded:TIMESTAMP#,'%Y-%m-%d %H %i')
     	]]>
 	 </select>
 	 
 	 <select id="getKeyLabelValueByMerchant" resultMap="CustomizedLabelValueBaseResultMap" parameterClass="com.mobicart.model.Labels">
     	<![CDATA[ 
     		select modified_text from labels WHERE merchant_id = #merchantId:BIGINT# 
     		AND label_key_id = (SELECT label_key_id from label_keys where label_key = #labelKey#)
     	]]>
 	 </select>
 	 
 	  <select id="getKeyLabelValueByDefault" resultMap="DefaultLabelValueBaseResultMap" parameterClass="com.mobicart.model.Labels">
     	<![CDATA[ 
