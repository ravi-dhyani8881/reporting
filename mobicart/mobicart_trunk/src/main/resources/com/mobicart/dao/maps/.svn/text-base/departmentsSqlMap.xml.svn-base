  <select id="selectCategoryByidAndMaxRowNum" resultMap="categoryMap" parameterClass="com.mobicart.model.Department" >   
     SELECT d.id, 
         d.parent_department_id, 
         d.s_name, 
         d.s_status,
         d.user_id, 
          ( 
         SELECT COUNT(id) FROM departments  WHERE parent_department_id =d.id   AND  s_status='active'
         
         ) iCategoryCount ,
       (
     SELECT  COUNT(*)  FROM (
				SELECT    (SELECT @rownum:=@rownum+1) rank,id,department_id				               				              
				FROM products,(SELECT @rownum:=0) r
				WHERE s_status!='hidden'  AND user_id=#userId# ORDER BY id  DESC  
				  ) temp 
     WHERE department_id= d.id AND  rank &lt;= ( 
											SELECT dsp.`i_max_limit` 
											FROM users usr 
											INNER JOIN `disk_space_pricing` dsp ON dsp.`id`=usr.`disk_space_pricing_id`
											WHERE usr.id= #userId#
   
					)    
         
				)  iProductCount
       
    FROM departments d WHERE  parent_department_id=#id# ORDER BY id DESC
     
  </select>
  
